{
  "index_name": "windicss-next",
  "start_urls": [
    {
      "url": "https://windicss.org/utilities/",
      "tags": ["utilities"]
    },
    {
      "url": "https://windicss.org/plugins/",
      "tags": ["plugins"]
    },
    {
      "url": "https://windicss.org/guide/",
      "tags": ["guide"]
    },
    {
      "url": "https://windicss.org/features/",
      "tags": ["features"]
    },
    {
      "url": "https://windicss.org/editors/",
      "tags": ["editors"]
    },
    {
      "url": "https://windicss.org/intergrations/",
      "tags": ["intergrations"]
    }
  ],
  "stop_urls": [
    ".html$"
  ],
  "selectors": {
    "default": {
      "lvl0": {
        "selector": ".sidebar h5.active",
        "global": true,
        "default_value": "Documentation"
      },
      "lvl1": ".content h1",
      "lvl2": ".content h2",
      "lvl3": ".content h3, .content li kbd",
      "lvl4": ".content h4, .content tbody tr td:nth-child(1)",
      "lvl5": ".content h5",
      "text": ".content p, .content li",
      "lang": {
        "selector": "/html/@lang",
        "type": "xpath",
        "global": true
      }
    }
  },
  "scrape_start_urls": true,
  "strip_chars": " .,;:#",
  "custom_settings": {
    "attributesForFaceting": [
      "lang"
    ]
  }
}
